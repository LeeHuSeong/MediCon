<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.medicon.medicon.controller.AdminPasswordController"
            stylesheets="@../../style/admin_password.css"
            prefWidth="400" prefHeight="250">
    <Label layoutX="120" layoutY="40" text="관리자 비밀번호 확인"/>
    <PasswordField fx:id="adminPwField"
                   layoutX="100" layoutY="90"
                   prefWidth="200"
                   promptText="비밀번호" />

    <Button fx:id="confirmButton"
            layoutX="100" layoutY="140"
            prefWidth="200"
            text="확인" />
</AnchorPane>
